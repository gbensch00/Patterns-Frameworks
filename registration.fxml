<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RegistrationController">
    <children>
        <Label layoutX="214.0" layoutY="31.0" text="Registrierung">
            
        </Label>
        <TextField fx:id="nameField" layoutX="122.0" layoutY="109.0" promptText="Enter username" />
        <PasswordField fx:id="passwordField" layoutX="122.0" layoutY="157.0" promptText="Enter password" />
        <Button fx:id="registerButton" layoutX="122.0" layoutY="207.0" mnemonicParsing="false" onAction="#handleRegisterButton" text="Register" />
        <Button fx:id="cancelButton" layoutX="252.0" layoutY="207.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Cancel" />
        <ImageView fx:id="defaultImage" layoutX="411.0" layoutY="109.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="infoLabel" layoutX="122.0" layoutY="277.0" text=" " />
    </children>
</AnchorPane>
